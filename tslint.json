{
    "extends": "tslint:latest",
    "rules": {
        "curly": true,
        "quotemark": [
            true, "single", "avoid-escape"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    },
    "jsRules": {
        "curly": true
    },
    "rulesDirectory": []
}